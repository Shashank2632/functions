#include<stdio.h> 
int sum(int n,int a[]);
int main()
{
    int n,i,a[100];
    printf("Enter the value of n");
    scanf("%d",&n);
    printf("Enter the values");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    printf("The sum is %d",sum(n,a));
    return 0;
    
}   
int sum(int n,int a[])
{
    int i,sum=0;
    for(i=0;i<n;i++)
    sum=sum+a[i];
    return sum;
}
